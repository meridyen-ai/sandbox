{
  "name": "@meridyen/sandbox-ui",
  "version": "0.1.0",
  "description": "React components for Meridyen Sandbox connection management",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tailwind-preset": "./src/tailwind/preset.js",
    "./icons/*": "./icons/*"
  },
  "files": [
    "dist",
    "icons",
    "src/tailwind"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "lucide-react": "^0.344.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/meridyen-ai/sandbox.git",
    "directory": "sdks/react"
  },
  "homepage": "https://github.com/meridyen-ai/sandbox"
}
